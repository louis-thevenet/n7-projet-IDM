<?xml version="1.0" encoding="UTF-8"?>
<calculus:Calculus
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:calculus="http://calculus"
    xsi:schemaLocation="http://calculus calculus.ecore"
    name="Mean">
  <Expressions
      xsi:type="calculus:InputsArgs"
      argName="maximum"/>
  <Expressions
      xsi:type="calculus:InputsArgs"
      argName="minimum"/>
  <Expressions
      xsi:type="calculus:Sum"
      First="//@Expressions.0"
      Second="//@Expressions.1"
      next="//@Expressions.4"
      name="Somme"
      op="+"/>
  <Expressions
      xsi:type="calculus:Constante"
      value="2.0"/>
  <Expressions
      xsi:type="calculus:Division"
      First="//@Expressions.2"
      Second="//@Expressions.3"
      next="//@Expressions.5"
      name="Division"
      op="/"/>
  <Expressions
      xsi:type="calculus:Result"
      result="resultat"
      from="//@Expressions.4"/>
</calculus:Calculus>
