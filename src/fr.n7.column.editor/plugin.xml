<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated column -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="Column.presentation.ColumnModelWizardID"
            name="%_UI_ColumnModelWizard_label"
            class="Column.presentation.ColumnModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ColumnModelFile.gif">
         <description>%_UI_ColumnModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated column -->
      <editor
            id="Column.presentation.ColumnEditorID"
            name="%_UI_ColumnEditor_label"
            icon="icons/full/obj16/ColumnModelFile.gif"
            extensions="column"
            class="Column.presentation.ColumnEditor"
            contributorClass="Column.presentation.ColumnActionBarContributor">
      </editor>
   </extension>

</plugin>
