<?xml version="1.0" encoding="UTF-8"?>
<chaiseMinute:ChaiseMinute
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:chaiseMinute="http://chaiseMinute"
    xmlns:function="http://function"
    name="exemple_sujet">
  <tables name="table">
    <columns
        xsi:type="chaiseMinute:ImportedColumn"
        id="Ouverture"
        type="Int"
        path="table.Open"/>
    <columns
        xsi:type="chaiseMinute:ImportedColumn"
        id="Min"
        type="Int"
        path="table.Low"/>
    <columns
        xsi:type="chaiseMinute:ImportedColumn"
        id="Max"
        type="Int"
        path="table.High"/>
    <columns
        xsi:type="chaiseMinute:ImportedColumn"
        id="Fermeture"
        type="Int"
        path="table.Close"/>
    <columns
        xsi:type="chaiseMinute:ColumnData"
        id="Volume"
        type="Int"/>
    <columns
        xsi:type="chaiseMinute:ComputedColumn"
        id="Moyenne"
        type="Int">
      <algorithm>
        <functions
            name="mean">
          <inputs
              xsi:type="function:ColumnArgument"
              columnPath="table.Min"/>
          <inputs
              xsi:type="function:ColumnArgument"
              columnPath="table.Max"/>
        </functions>
      </algorithm>
    </columns>
    <columns
        xsi:type="chaiseMinute:ComputedColumn"
        id="Variation"
        type="Int">
      <algorithm>
        <functions
            name="variation">
          <inputs
              xsi:type="function:ColumnArgument"
              columnPath="table.Fermeture"/>
          <inputs
              xsi:type="function:ColumnArgument"
              columnPath="table.Ouverture"/>
        </functions>
      </algorithm>
    </columns>
    <indexColumn
        id="Date"/>
    <constraints>
      <functions
          name="ensure_min_max">
        <inputs
            xsi:type="function:ColumnArgument"
            columnPath="table.Min"/>
        <inputs
            xsi:type="function:ColumnArgument"
            columnPath="table.Max"/>
      </functions>
    </constraints>
  </tables>
</chaiseMinute:ChaiseMinute>
