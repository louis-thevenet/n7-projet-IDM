<?xml version="1.0" encoding="UTF-8"?>
<chaiseMinute:ChaiseMinute
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:chaiseMinute="http://chaiseMinute"
    xmlns:function="http://function"
    xsi:schemaLocation="http://function ../fr.n7.chaiseMinute.function/function.ecore">
  <tables name="Example1">
    <columns
        xsi:type="chaiseMinute:ColumnData"
        id="A"
        type="Int"/>
    <columns
        xsi:type="chaiseMinute:ColumnData"
        id="B"
        type="Int"/>
    <columns
        xsi:type="chaiseMinute:ComputedColumn"
        id="C"
        type="Int"
        output="//@tables.0/@columns.2/@algorithm/@inputs.2">
      <algorithm>
        <inputs
            xsi:type="function:ColumnArgument"
            columnPath="Table1.A"/>
        <inputs
            xsi:type="function:ColumnArgument"
            columnPath="Table1.B"/>
        <inputs
            xsi:type="function:ColumnArgument"
            columnPath="Table1.C"/>
        <functions>Sum</functions>
      </algorithm>
    </columns>
  </tables>
</chaiseMinute:ChaiseMinute>
