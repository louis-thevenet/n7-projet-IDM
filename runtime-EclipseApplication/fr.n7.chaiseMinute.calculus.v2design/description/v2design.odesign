                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="calculusv2::Pipe" referenceName="elements">
                  <subModelOperations xsi:type="tool_1:If" conditionExpression="[self.targetPipe.before = null/]">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="targetPipe" valueExpression="[target/]"/>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool_1:If" conditionExpression="[self.targetPipe.oclIsTypeOf(BinaryOperation) and self.targetPipe.before &lt;> null/]">
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[target/]">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="beforeSecond" valueExpression="[target/]"/>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="sourcePipe" valueExpression="[source/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
